#foreach pack ( cutest_ureport )
#foreach pack ( cutest_usetup )
#foreach pack ( cutest_ush )
#foreach pack ( cutest_uvartype )
#foreach pack ( cutest_varnames )
  echo "    " $pack
  echo "      .. making pdf"
  pdfroff -e -m man $pack.3 > ../../doc/pdf/$pack.pdf
  echo "      .. making html"
  groff -e -m man -T html -P "-Istem $pack-" -P "-i150" $pack.3 \
    > /numerical/nimg/fortran/cutest.svn/doc/html/$pack.html
  mv *.png /numerical/nimg/fortran/cutest.svn/doc/html
end


